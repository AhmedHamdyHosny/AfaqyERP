
@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_Popup.cshtml";
}

<div class="modal-header">
    <button type="button" class="close" ng-click="cancel()" aria-hidden="true">&times;</button>
    <h2 class="modal-title">Report</h2> @*@Resources.Store.DeliveryNoteReport*@
</div>
@using (Html.BeginForm())
{
    <div class="modal-body withVScroll">
        <div class="row">
            <div class="col-xs-12 form-group">
                <iframe src="~/Reports/Store/DeliveryNoteDeviceReportViewer.aspx?DeliveryId=@ViewBag.Id" frameborder="0" style="width: 100%;" height="1000" scrolling="no"></iframe>
            </div>
        </div>
        
        
    </div>
    <div class="modal-footer">
        <button class="btn btn-success" type="button" ng-click="cancel()">@Resources.Resource.ButtonOk</button>
    </div>
}
