@model Afaqy_Store.DataLayer.SIMCardStatus

@{
    ViewBag.Title = Resources.Store.SIMCardStatusDetails;
    Layout = "~/Views/Shared/_Popup.cshtml";
}

<div class="modal-header">
    <button type="button" class="close" ng-click="cancel()" aria-hidden="true">&times;</button>
    <h2 class="modal-title">@Resources.Store.SIMCardStatusDetails</h2>
</div>

@using (Html.BeginForm())
{
    <div class="modal-body">
        <dl class="dl-horizontal">
            <dt>
               @Resources.Store.SIMCardStatusNameEn
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SIMCardStatusName_en)
            </dd>
            <dt>
               @Resources.Store.SIMCardStatusNameAr
            </dt>

            <dd>
                @Html.DisplayFor(model => model.SIMCardStatusName_ar)
            </dd>
        </dl>
    </div>
    <div class="modal-footer">
        <button class="btn btn-success" type="button" ng-click="cancel()">@Resources.Resource.ButtonOk</button>
    </div>
}
